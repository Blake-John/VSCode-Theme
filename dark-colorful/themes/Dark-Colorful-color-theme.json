				"constant.character",
				"constant.other",
				"constant.language"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#CB4B16"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				// "punctuation",
				"meta.block"
			],
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "right left shift",
			"scope": [
				"keyword.operator.bitwise.shift",
				"keyword.operator.bitwise.shift",
			],
			"settings": {
